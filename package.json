{
  "name": "css-framework",
  "version": "1.0.0",
  "description": "CSS framework and project setup for social media app",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
  "build": "sass src/scss:dist/css && cp -R src/js dist/js && cp -R src/svg dist/svg",
  "watch": "sass --watch src/scss:dist/css & live-server dist",
  "start": "npm run build && npm run watch",
    "clean": "rm -rf dist",
    "lint": "eslint src/js",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "keywords": [
    "css",
    "framework",
    "sass",
    "bootstrap",
    "social-media"
  ],
  "author": "fantadiallo",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^5.3.3"
  },
  "devDependencies": {
    "eslint": "^9.14.0",
    "gh-pages": "^6.2.0",
    "live-server": "^1.2.2",
    "sass": "^1.80.6"
  }
}
